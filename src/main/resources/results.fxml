<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="general.ResultsController">
   <children>
      <ImageView fx:id="vCongratsImage" fitHeight="150.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </ImageView>
      <Label fx:id="vSummarySteps" text=" You coped with the task in 10 steps">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="vSummaryTime" text="Elapsed time - 15 minutes">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button id="got-it-button" fx:id="vNewGameButton" alignment="CENTER" maxWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#startNewGame" prefWidth="105.0" stylesheets="@styles.css" text="New Game!" textAlignment="CENTER" textFill="WHITE">
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin></Button>
      <Button id="got-it-button" fx:id="vQuitButton" alignment="CENTER" layoutX="267.0" layoutY="296.0" maxWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#quit" prefWidth="105.0" stylesheets="@styles.css" text="Quit" textAlignment="CENTER" textFill="WHITE">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
